# ABOUTME: Example environment variables for development
# ABOUTME: Copy to .env and update with your values

# AWS Configuration
AWS_REGION=us-west-2
AWS_ACCOUNT_ID=123456789012

# Bedrock Configuration
BEDROCK_MODEL_ID=anthropic.claude-3-5-sonnet-20241022-v2:0

# ECS Configuration
ECS_CLUSTER=computer-use-dev
TASK_DEFINITION=computer-use-desktop
SUBNETS=subnet-xxx,subnet-yyy,subnet-zzz
SECURITY_GROUP=sg-xxxxxxxxx
TARGET_GROUP_ARN=arn:aws:elasticloadbalancing:region:account:targetgroup/xxx

# DynamoDB Tables
SESSION_TABLE=computer-use-sessions-dev
AGENT_CACHE_TABLE=computer-use-agent-cache-dev

# S3 Buckets
S3_BUCKET=computer-use-assets-dev-123456789012
FRONTEND_BUCKET=computer-use-frontend-dev-123456789012

# VNC Configuration
VNC_HOST=localhost
VNC_PORT=5900
VNC_PASSWORD=changeme

# API Endpoints
API_ENDPOINT=https://api.dev.computer-use.example.com
WS_ENDPOINT=wss://ws.dev.computer-use.example.com

# Development Settings
DEBUG=true
LOG_LEVEL=INFO