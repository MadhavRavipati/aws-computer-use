# ABOUTME: Backend-specific environment variables
# ABOUTME: Copy to .env and update with your values

# AWS Configuration
AWS_REGION=us-west-2

# Bedrock Configuration
BEDROCK_MODEL_ID=anthropic.claude-3-5-sonnet-20241022-v2:0

# DynamoDB Tables
DYNAMODB_TABLE=computer-use-sessions-dev
CACHE_TABLE=computer-use-agent-cache-dev

# S3 Configuration
S3_BUCKET=computer-use-assets-dev-123456789012

# VNC Bridge Configuration
VNC_HOST=localhost
VNC_PORT=5900
VNC_PASSWORD=changeme

# Session Configuration
SESSION_ID=test-session-id

# Lambda Configuration
ECS_CLUSTER=computer-use-dev
ECS_SERVICE=desktop-service
TASK_DEFINITION=computer-use-desktop:latest
SUBNETS=subnet-xxx,subnet-yyy
SECURITY_GROUP=sg-xxxxxxxxx
SESSION_TABLE=computer-use-sessions-dev
TARGET_GROUP_ARN=arn:aws:elasticloadbalancing:region:account:targetgroup/xxx

# Development
PYTHONPATH=.
LOG_LEVEL=INFO